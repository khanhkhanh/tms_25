<% provide :title, t(:edit_course) %>
<div class="row" >
  <div class="col-md-6 col-md-offset-3" >
    <h1>Edit the course</h1>
    <%= form_for [:supervisor, @course] do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'form', f: f %>
    <% end %>
  </div>

  <div class="col-md-6 col-md-offset-3" >
    <br><br><br>
    <h1>Add subjects to the course</h1>
    <%= bootstrap_form_for [:supervisor, @course] do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'add_subjects', f: f %>
    <% end %>
  </div>
</div>
